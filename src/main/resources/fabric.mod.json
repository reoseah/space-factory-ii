{
  "schemaVersion": 1,
  "id": "spacefactory",
  "version": "${version}",
  "name": "Example mod",
  "description": "This is an spacefactory description! Tell everyone what your mod is about!",
  "authors": [
    "Me!"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-spacefactory-mod"
  },
  "license": "CC0-1.0",
  "icon": "assets/spacefactory/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.spacefactory.SpaceFactory::initialize"
    ],
    "client": [
      "com.spacefactory.SpaceFactoryClient::initialize"
    ]
  },
  "mixins": [
    "spacefactory.mixins.json",
    {
      "config": "spacefactory.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.14.21",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}